{
  "name": "@supercollider.js/logger",
  "version": "1.0.0",
  "description": "Console logging utility for supercollider.js with colors",
  "keywords": [
    "supercollider"
  ],
  "author": "Chris Sattinger <crucialfelix@gmail.com>",
  "homepage": "https://crucialfelix.github.io/supercolliderjs/",
  "license": "MIT",
  "main": "lib/logger.js",
